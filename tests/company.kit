with Localisation;
with Company_Objects;

package Company is

for Company'Database_Package_Name use "Company.Db";
for company'Handle_Package_Name use "Company.Handles";

record Inventory : Object;

record Resource : Object;

record Employee : Resource;

record Product_Group : Inventory is
   key Manager : Employee;
end Product_Group;
  
record Product is
   key Product_Group;
end Product;

end Company;
